{
	"name": "kent",
	"version": "1.0.0",
	"license": "Apache-2.0",
	"author": "Renan Sigolo <renan.sigolo@gmail.com>",
	"scripts": {
		"build-prod": "ng build --prod",
		"build": "ng build",
		"clean": "rm -r dist node_modules yarn.lock package-lock.json",
		"deploy-functions": "firebase deploy --only functions",
		"deploy": "firebase deploy",
		"lint-fix": "yarn lint-js-fix && yarn lint-scss-fix && yarn prettier",
		"lint-js-fix": "ng lint kent --format stylish --fix",
		"lint-js": "ng lint kent --format stylish",
		"lint-scss-fix": "stylelint \"src/**/*.scss\" --fix",
		"lint-scss": "stylelint \"src/**/*.scss\"",
		"lint": "yarn lint-js && yarn lint-scss",
		"precommit": "yarn prettier && git add .",
		"prepush": "yarn test",
		"prettier": "prettier --config ./.prettierrc.json --write \"{src,functions}/**/*.{ts,js,json,scss}\"",
		"serve": "ng serve -o",
		"test": "yarn lint-fix && yarn build-prod",
		"update": "yarn upgrade-interactive --latest",
		"e2e": "cypress open"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^6.0.9",
		"@angular/cdk": "^6.3.3",
		"@angular/common": "^6.0.9",
		"@angular/compiler": "^6.0.9",
		"@angular/core": "^6.0.9",
		"@angular/forms": "^6.0.9",
		"@angular/http": "^6.0.9",
		"@angular/material": "^6.3.3",
		"@angular/platform-browser": "^6.0.9",
		"@angular/platform-browser-dynamic": "^6.0.9",
		"@angular/pwa": "^0.6.8",
		"@angular/router": "^6.0.9",
		"@angular/service-worker": "^6.0.9",
		"angularfire2": "5.0.0-rc.11",
		"blueimp-canvas-to-blob": "^3.14.0",
		"bootstrap": "4.1.2",
		"core-js": "^2.5.7",
		"firebase": "^5.2.0",
		"ng2-img-tools": "^1.1.3",
		"ngx-bootstrap": "^3.0.1",
		"ngx-toastr": "^8.8.0",
		"npm": "^6.2.0",
		"rxjs": "^6.2.2",
		"rxjs-compat": "^6.2.2",
		"web-animations-js": "^2.3.1",
		"zone.js": "^0.8.26"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.6.8",
		"@angular/cli": "~6.0.8",
		"@angular/compiler-cli": "^6.0.9",
		"@angular/language-service": "^6.0.9",
		"@types/node": "^10.5.2",
		"chance": "^1.0.16",
		"codelyzer": "~4.4.2",
		"cypress": "^3.0.2",
		"husky": "0.15.0-rc.13",
		"jasmine-core": "~3.1.0",
		"jasmine-spec-reporter": "~4.2.1",
		"prettier": "^1.13.7",
		"stylelint": "^9.3.0",
		"stylelint-config-sass-guidelines": "^5.0.0",
		"stylelint-scss": "^3.1.3",
		"ts-node": "~7.0.0",
		"tslint": "~5.11.0",
		"typescript": "2.7.2"
	}
}
