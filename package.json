{
	"name": "kent",
	"version": "1.0.0",
	"license": "Apache-2.0",
	"author": "Renan Sigolo <renan.sigolo@gmail.com>",
	"scripts": {
		"build-prod": "ng build --prod",
		"build": "ng build",
		"clean": "rm -r dist node_modules yarn.lock package-lock.json",
		"deploy-functions": "firebase deploy --only functions",
		"deploy": "firebase deploy",
		"e2e": "ng e2e",
		"lint-fix": "yarn lint-js-fix && yarn lint-scss-fix && yarn prettier",
		"lint-js-fix": "ng lint kent --format stylish --fix  && ng lint kent-e2e --fix",
		"lint-js": "ng lint kent --format stylish && ng lint kent-e2e",
		"lint-scss-fix": "stylelint \"src/**/*.scss\" --fix",
		"lint-scss": "stylelint \"src/**/*.scss\"",
		"lint": "yarn lint-js && yarn lint-scss",
		"precommit": "yarn prettier && git add .",
		"prepush": "yarn test",
		"prettier": "prettier --config ./.prettierrc.json --write \"{src,e2e,functions}/**/*.{ts,js,json,scss}\"",
		"serve": "ng serve -o",
		"test": "yarn lint-fix && yarn build-prod",
		"update": "yarn upgrade-interactive --latest"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^6.0.7",
		"@angular/cdk": "^6.3.2",
		"@angular/common": "^6.0.7",
		"@angular/compiler": "^6.0.7",
		"@angular/core": "^6.0.7",
		"@angular/forms": "^6.0.7",
		"@angular/http": "^6.0.7",
		"@angular/material": "^6.3.2",
		"@angular/platform-browser": "^6.0.7",
		"@angular/platform-browser-dynamic": "^6.0.7",
		"@angular/pwa": "^0.6.8",
		"@angular/router": "^6.0.7",
		"@angular/service-worker": "^6.0.7",
		"angularfire2": "5.0.0-rc.11",
		"blueimp-canvas-to-blob": "^3.14.0",
		"bootstrap": "4.1.1",
		"core-js": "^2.5.7",
		"firebase": "^5.2.0",
		"ng2-img-tools": "^1.1.3",
		"ngx-bootstrap": "^3.0.1",
		"ngx-toastr": "^8.8.0",
		"npm": "^6.1.0",
		"rxjs": "^6.2.1",
		"rxjs-compat": "^6.2.1",
		"web-animations-js": "^2.3.1",
		"zone.js": "^0.8.26"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.6.8",
		"@angular/cli": "~6.0.8",
		"@angular/compiler-cli": "^6.0.7",
		"@angular/language-service": "^6.0.7",
		"@types/jasmine": "~2.8.8",
		"@types/jasminewd2": "~2.0.3",
		"@types/node": "^10.5.2",
		"codelyzer": "~4.4.2",
		"husky": "0.15.0-rc.13",
		"jasmine-core": "~3.1.0",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "~2.0.4",
		"karma-chrome-launcher": "~2.2.0",
		"karma-coverage-istanbul-reporter": "~2.0.1",
		"karma-jasmine": "~1.1.2",
		"karma-jasmine-html-reporter": "^1.2.0",
		"prettier": "^1.13.7",
		"protractor": "~5.3.2",
		"stylelint": "^9.3.0",
		"stylelint-config-sass-guidelines": "^5.0.0",
		"stylelint-scss": "^3.1.3",
		"ts-node": "~7.0.0",
		"tslint": "~5.10.0",
		"typescript": "2.7.2"
	}
}
