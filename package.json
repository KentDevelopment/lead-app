{
  "name": "kent",
  "version": "1.3.6",
  "license": "Apache-2.0",
  "author": "Renan Sigolo <renan.sigolo@gmail.com>",
  "private": false,
  "scripts": {
    "build:prod": "ng build --prod",
    "build": "ng build",
    "clean": "rm -rf node_modules dist package-lock.json yarn.lock",
    "deploy": "npm run build:prod && firebase deploy",
    "e2e:run": "cypress run",
    "e2e": "cypress open",
    "lint:fix": "npm run lint:ts && npm run lint:styles && npm run prettier",
    "lint:styles": "stylelint \"src/**/*.scss\" --fix",
    "lint:ts": "ng lint kent --format stylish --fix",
    "lint": "npm run lint:ts && npm run lint:styles",
    "prettier": "prettier --config ./.prettierrc.json --write '**/*.{ts,js,json,yml,html,md}'",
    "start": "ng serve -o",
    "serve": "ng serve -o --host=0.0.0.0",
    "test": "npm run lint:fix && npm run build:prod"
  },
  "dependencies": {
    "@angular/animations": "^7.0.4",
    "@angular/cdk": "^7.0.4",
    "@angular/common": "^7.0.4",
    "@angular/compiler": "^7.0.4",
    "@angular/core": "^7.0.4",
    "@angular/fire": "^5.1.0",
    "@angular/forms": "^7.0.4",
    "@angular/http": "^7.0.4",
    "@angular/material": "^7.0.4",
    "@angular/platform-browser": "^7.0.4",
    "@angular/platform-browser-dynamic": "^7.0.4",
    "@angular/pwa": "^0.10.6",
    "@angular/router": "^7.0.4",
    "@angular/service-worker": "^7.0.4",
    "blueimp-canvas-to-blob": "^3.14.0",
    "bootstrap": "4.1.3",
    "core-js": "^2.5.7",
    "firebase": "^5.5.8",
    "ng2-img-tools": "^1.1.5",
    "npm": "^6.4.1",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.6",
    "@angular/cli": "~7.0.6",
    "@angular/compiler-cli": "^7.0.4",
    "@angular/language-service": "^7.0.4",
    "@types/node": "^10.12.9",
    "chance": "^1.0.16",
    "codelyzer": "~4.5.0",
    "cypress": "^3.1.2",
    "husky": "1.1.4",
    "prettier": "^1.15.2",
    "stylelint": "^9.8.0",
    "stylelint-config-sass-guidelines": "^5.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.4.0",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "tslint-angular": "^1.1.2",
    "tslint-config-prettier": "^1.16.0",
    "typescript": "3.1.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier && git add .",
      "pre-push": "npm run test"
    }
  }
}
