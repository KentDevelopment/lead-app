{
  "name": "kent",
  "version": "1.2.1",
  "license": "Apache-2.0",
  "author": "Renan Sigolo <renan.sigolo@gmail.com>",
  "scripts": {
    "build-prod": "ng build --prod",
    "build": "ng build",
    "clean": "rm -r dist node_modules yarn.lock package-lock.json",
    "deploy-functions": "firebase deploy --only functions",
    "deploy": "firebase deploy",
    "e2e": "cypress open",
    "lint-fix": "yarn lint-js-fix && yarn lint-scss-fix && yarn prettier",
    "lint-js-fix": "ng lint kent --format stylish --fix",
    "lint-js": "ng lint kent --format stylish",
    "lint-scss-fix": "stylelint \"src/**/*.scss\" --fix",
    "lint-scss": "stylelint \"src/**/*.scss\"",
    "lint": "yarn lint-js && yarn lint-scss",
    "prettier": "prettier --config ./.prettierrc.json --write \"**/*.{ts,js,json,scss,yml}\"",
    "run-e2e": "cypress run",
    "serve": "ng serve -o",
    "test": "yarn lint-fix && yarn build-prod",
    "update": "yarn upgrade-interactive --latest"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.9",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^6.1.9",
    "@angular/compiler": "^6.1.9",
    "@angular/core": "^6.1.9",
    "@angular/fire": "^5.0.2",
    "@angular/forms": "^6.1.9",
    "@angular/http": "^6.1.9",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^6.1.9",
    "@angular/platform-browser-dynamic": "^6.1.9",
    "@angular/pwa": "^0.8.4",
    "@angular/router": "^6.1.9",
    "@angular/service-worker": "^6.1.9",
    "blueimp-canvas-to-blob": "^3.14.0",
    "bootstrap": "4.1.3",
    "core-js": "^2.5.7",
    "firebase": "^5.5.3",
    "ng2-img-tools": "^1.1.5",
    "npm": "^6.4.1",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.4",
    "@angular/cli": "~6.2.4",
    "@angular/compiler-cli": "^6.1.9",
    "@angular/language-service": "^6.1.9",
    "@types/node": "^10.11.6",
    "chance": "^1.0.16",
    "codelyzer": "~4.5.0",
    "cypress": "^3.1.0",
    "husky": "1.1.1",
    "prettier": "^1.14.3",
    "stylelint": "^9.6.0",
    "stylelint-config-sass-guidelines": "^5.2.0",
    "stylelint-scss": "^3.3.1",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "2.9.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier && git add .",
      "pre-push": "yarn test"
    }
  }
}
