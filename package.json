{
  "name": "kent",
  "version": "1.3.2",
  "license": "Apache-2.0",
  "author": "Renan Sigolo <renan.sigolo@gmail.com>",
  "private": true,
  "scripts": {
    "build:prod": "ng build --prod",
    "build": "ng build",
    "clean": "rm -r dist node_modules yarn.lock package-lock.json",
    "deploy": "yarn build:prod && firebase deploy",
    "e2e:run": "cypress run",
    "e2e": "cypress open",
    "lint:fix": "yarn lint:ts --fix && yarn lint:styles --fix && yarn prettier",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "lint:ts": "ng lint kent --format stylish",
    "lint": "yarn lint:ts && yarn lint:styles",
    "prettier": "prettier --config ./.prettierrc.json --write \"**/*.{ts,js,json,yml}\"",
    "start": "ng serve -o",
    "test": "yarn lint:fix && yarn build:prod",
    "update": "yarn upgrade-interactive --latest"
  },
  "dependencies": {
    "@angular/animations": "^7.0.1",
    "@angular/cdk": "^7.0.2",
    "@angular/common": "^7.0.1",
    "@angular/compiler": "^7.0.1",
    "@angular/core": "^7.0.1",
    "@angular/fire": "^5.1.0",
    "@angular/forms": "^7.0.1",
    "@angular/http": "^7.0.1",
    "@angular/material": "^7.0.2",
    "@angular/platform-browser": "^7.0.1",
    "@angular/platform-browser-dynamic": "^7.0.1",
    "@angular/pwa": "^0.10.3",
    "@angular/router": "^7.0.1",
    "@angular/service-worker": "^7.0.1",
    "blueimp-canvas-to-blob": "^3.14.0",
    "bootstrap": "4.1.3",
    "core-js": "^2.5.7",
    "firebase": "^5.5.6",
    "ng2-img-tools": "^1.1.5",
    "npm": "^6.4.1",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.3",
    "@angular/cli": "~7.0.3",
    "@angular/compiler-cli": "^7.0.1",
    "@angular/language-service": "^7.0.1",
    "@types/node": "^10.12.1",
    "chance": "^1.0.16",
    "codelyzer": "~4.5.0",
    "cypress": "^3.1.0",
    "husky": "1.1.2",
    "prettier": "^1.14.3",
    "stylelint": "^9.7.1",
    "stylelint-config-sass-guidelines": "^5.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.3.2",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "tslint-angular": "^1.1.2",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "3.1.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier && git add .",
      "pre-push": "yarn test"
    }
  }
}
