{
  "name": "lead-app",
  "version": "2.0.0",
  "description": "Lead Points App",
  "homepage": "https://lead.kent.edu.au/",
  "bugs": {
    "url": "https://github.com/KentDevelopment/lead-app/issues",
    "email": "renan.sigolo@gmail.com"
  },
  "license": "Apache-2.0",
  "author": "Renan Sigolo <renan.sigolo@gmail.com>",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/KentDevelopment/lead-app.git"
  },
  "scripts": {
    "preversion": "git checkout master && git merge origin/develop && npm run test",
    "postversion": "git push && git push --tags && git checkout develop && git merge origin/master && git push",
    "build:prod": "ng build --prod",
    "build": "ng build",
    "clean": "rm -rf node_modules package-lock.json dist",
    "clean:install": "rm -rf node_modules package-lock.json dist && npm i",
    "deploy": "npm run build:prod && firebase deploy",
    "deploy:dev": "npm run build:prod && firebase deploy -P development",
    "e2e:run": "cypress run",
    "e2e": "cypress open",
    "lint:fix": "ng lint --fix && stylelint \"src/**/*.scss\" --fix && npm run prettier",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "lint:ts": "ng lint --format stylish",
    "lint": "npm run lint:ts && npm run lint:styles",
    "prettier": "prettier --config ./.prettierrc.json --write \"./**/*.{js,jsx,ts,tsx,json,css,scss,md,component.html,yml}\"",
    "serve": "ng serve -o --host=0.0.0.0",
    "start": "ng serve -o",
    "test": "npm run lint && npm run build:prod"
  },
  "dependencies": {
    "@angular/animations": "^7.2.0",
    "@angular/cdk": "^7.2.1",
    "@angular/common": "^7.2.0",
    "@angular/compiler": "^7.2.0",
    "@angular/core": "^7.2.0",
    "@angular/fire": "^5.1.1",
    "@angular/flex-layout": "^7.0.0-beta.22",
    "@angular/forms": "^7.2.0",
    "@angular/http": "^7.2.0",
    "@angular/material": "^7.2.1",
    "@angular/platform-browser": "^7.2.0",
    "@angular/platform-browser-dynamic": "^7.2.0",
    "@angular/pwa": "^0.12.1",
    "@angular/router": "^7.2.0",
    "@angular/service-worker": "^7.2.0",
    "blueimp-canvas-to-blob": "^3.14.0",
    "bootstrap": "4.2.1",
    "chance": "^1.0.18",
    "core-js": "^2.6.1",
    "firebase": "^5.7.2",
    "lint-staged": "^8.1.0",
    "ng2-img-tools": "^1.1.5",
    "ngx-image-cropper": "^1.3.5",
    "npm": "^6.5.0",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "tslib": "^1.9.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.1",
    "@angular/cli": "^7.2.1",
    "@angular/compiler-cli": "^7.2.0",
    "@angular/language-service": "^7.2.0",
    "@types/node": "^10.12.18",
    "codelyzer": "~4.5.0",
    "cypress": "^3.1.4",
    "husky": "^1.3.1",
    "prettier": "^1.15.3",
    "stylelint": "^9.9.0",
    "stylelint-config-sass-guidelines": "^5.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.4.4",
    "ts-node": "~7.0.1",
    "tslint": "~5.12.0",
    "tslint-angular": "^1.1.2",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "3.2.2"
  },
  "engines": {
    "node": "~10.13.0",
    "npm": ">= 6.5.0",
    "yarn": "NO LONGER USED - Please use npm"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier && git add .",
      "pre-push": "npm run lint:fix"
    }
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx,json,css,scss,md,component.html,yml}": [
      "prettier --config ./.prettierrc.json --write",
      "git add"
    ]
  }
}
